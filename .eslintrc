{
  "extends": [
    "react-app",
    "airbnb-base",
    "prettier"
    ],
  "env": {
    "browser": true,
    "node": true,
    "es6": true,
    "jest": true
  },
  "plugins": [
    "prettier"
  ],
  "root": true,
  "rules": {
    "prettier/prettier": "error",
    "linebreak-style": "off",
    "radix": "off",
    "consistent-return": "off"
  }
}
