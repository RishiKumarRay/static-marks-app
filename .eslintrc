{
  "extends": ["react-app", "prettier"],
  "env": {
    "browser": true,
    "es6": true,
    "jest": true,
    "node": true
  },
  "plugins": ["import", "jsx-a11y", "prettier", "react"],
  "root": true,
  "rules": {
    "linebreak-style": "off",
    "prettier/prettier": "error",
    "radix": "off",
    "consistent-return": "off",
    "react/jsx-filename-extension": "off"
  }
}
