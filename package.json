{
  "name": "static-marks-app",
  "version": "1.0.0",
  "dependencies": {
    "classnames": "2.2.6",
    "faker": "4.1.0",
    "inline-source": "6.1.5",
    "prop-types": "15.6.2",
    "react": "16.4.1",
    "react-dom": "16.4.1",
    "react-router-dom": "4.3.1",
    "react-scripts": "1.1.4",
    "why-did-you-update": "0.1.1"
  },
  "devDependencies": {
    "babel-eslint": "7.2.3",
    "eslint": "4.19.1",
    "eslint-config-prettier": "2.9.0",
    "eslint-plugin-import": "2.12.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-prettier": "2.6.0",
    "eslint-plugin-react": "7.9.1",
    "prettier": "1.13.5"
  },
  "private": true,
  "scripts": {
    "build": "react-scripts build && node scripts/inline-sources.js",
    "ci": "npm run lint:check && npm run test",
    "format": "prettier --write -- \"{src,public}/**/*.{js,jsx,json,css}\"",
    "inline-sources": "node scripts/inline-sources.js",
    "lint:check": "eslint src/**/*.js",
    "lint:fix": "eslint --fix src/**/*.js",
    "start": "react-scripts start",
    "test": "react-scripts test --env=jsdom --modulePaths=src"
  }
}
