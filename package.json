{
  "name": "static-marks-app",
  "version": "1.0.0",
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "dependencies": {
    "classnames": "2.2.6",
    "faker": "4.1.0",
    "inline-source": "6.1.8",
    "prop-types": "15.6.2",
    "react": "16.6.1",
    "react-dom": "16.6.1",
    "react-router-dom": "4.3.1",
    "react-scripts": "2.1.1",
    "why-did-you-update": "1.0.5"
  },
  "devDependencies": {
    "cross-env": "5.2.0",
    "eslint-config-prettier": "3.1.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx-a11y": "6.1.2",
    "eslint-plugin-prettier": "3.0.0",
    "eslint-plugin-react": "7.11.1",
    "node-sass": "4.10.0",
    "prettier": "1.15.1"
  },
  "private": true,
  "scripts": {
    "build": "react-scripts build && node scripts/inline-sources.js",
    "ci": "npm run lint:check && npm run test",
    "format": "prettier --write -- \"{src,public}/**/*.{js,jsx,json,css}\"",
    "inline-sources": "node scripts/inline-sources.js",
    "lint:check": "eslint src/**/*.js",
    "lint:fix": "eslint --fix src/**/*.js",
    "start": "react-scripts start",
    "test": "cross-env CI=true react-scripts test --env=jsdom --modulePaths=src"
  }
}
