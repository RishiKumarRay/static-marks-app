{
  "name": "static-marks-app",
  "description": "Frontend for Static Marks",
  "version": "2.2.7",
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "dependencies": {
    "@darekkay/styles": "1.0.11",
    "classnames": "2.2.6",
    "inline-source": "7.2.0",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-router-dom": "5.2.0",
    "react-scripts": "3.4.3"
  },
  "devDependencies": {
    "@darekkay/eslint-config": "2.1.2",
    "cross-env": "7.0.2",
    "eslint-config-prettier": "6.12.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jest": "23.20.0",
    "eslint-plugin-jest-dom": "3.2.3",
    "eslint-plugin-jsx-a11y": "6.3.1",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-react": "7.21.2",
    "eslint-plugin-react-hooks": "4.1.2",
    "eslint-plugin-testing-library": "3.9.0",
    "eslint-plugin-unicorn": "21.0.0",
    "faker": "5.1.0",
    "node-sass": "4.14.1",
    "prettier": "2.1.2"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "react-scripts build && node scripts/inline-sources.js",
    "ci": "npm run lint && npm run test",
    "format": "prettier --write .",
    "inline-sources": "node scripts/inline-sources.js",
    "lint": "eslint src/**/*.js",
    "lint:fix": "eslint --fix src/**/*.js",
    "start": "react-scripts start",
    "test": "cross-env CI=true react-scripts test --env=jsdom --modulePaths=src"
  }
}
